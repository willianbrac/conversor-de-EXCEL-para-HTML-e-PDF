<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vers√£o WEB</title>
</head>
<body>
    <table>
        <thead>
            <% header.forEach(column =>{ %>
                <th><%- column %> </th>                
            <% }); %> 
        </thead>
        <tbody>
            <% rows.forEach(row =>{ %>
                <tr> 
                    <% row.forEach(element => { %>
                       <td> <%- element %></td>
                    <% }); %>  
                </tr>                
            <% }); %> 
        </tbody>
    </table>
</body>
</html>